db.booking.aggregate([
    {
      $lookup: {
        from: "user",
        localField: "customer.id",   // Field in bookings
        foreignField: "id",          // Field in users
        as: "userMatch"
      }
    },
    {
      $set: {
        "customer.userid": { $arrayElemAt: ["$userMatch._id", 0] }
      }
    },
    {
      $unset: "userMatch"
    },
    {
      $merge: {
        into: "booking",
        on: "_id",
        whenMatched: "merge",
        whenNotMatched: "fail"
